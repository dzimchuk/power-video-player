    <ControlTemplate x:Key="MainButtonTemplate" TargetType="{x:Type Button}">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0:0:0.2" To="Normal"/>
                        <VisualTransition From="Normal" GeneratedDuration="0:0:0.2" To="MouseOver"/>
                        <VisualTransition From="Pressed" GeneratedDuration="0" To="MouseOver"/>
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="HighlightBackground">
                                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Effect).(DropShadowEffect.Opacity)" Storyboard.TargetName="Outline">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.3"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Glow">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.85"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="PressedBackground">
                                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Effect).(DropShadowEffect.Opacity)" Storyboard.TargetName="Outline">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.3"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="contentPresenter">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.9"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" Storyboard.TargetName="contentPresenter">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.9"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="DisabledBackground">
                                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="contentPresenter">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.4"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Glow">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.3"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Ellipse x:Name="Outline" StrokeThickness="1" SnapsToDevicePixels="True">
                <Ellipse.Effect>
                    <DropShadowEffect ShadowDepth="1" BlurRadius="2" Opacity="1"/>
                </Ellipse.Effect>
                <Ellipse.Stroke>
                    <LinearGradientBrush EndPoint="0.146,0.146" StartPoint="0.854,0.854">
                        <GradientStop Color="#4C000000" Offset="0"/>
                        <GradientStop Color="#33FFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Ellipse.Stroke>
            </Ellipse>
            <Ellipse x:Name="RegularBackground" >
                <Ellipse.Fill>
                    <RadialGradientBrush GradientOrigin="0.883,0.86">
                        <GradientStop Color="#FF919191" Offset="0.126"/>
                        <GradientStop Color="#FFE3E3E3" Offset="1"/>
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Ellipse x:Name="HighlightBackground" Opacity="0" >
                <Ellipse.Fill>
                    <RadialGradientBrush GradientOrigin="0.883,0.86">
                        <GradientStop Color="#FFD88956" Offset="0.126"/>
                        <GradientStop Color="#FFFBF4C0" Offset="1"/>
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Ellipse x:Name="PressedBackground" Opacity="0" >
                <Ellipse.Fill>
                    <RadialGradientBrush GradientOrigin="0.883,0.86">
                        <GradientStop Color="#FFDA905E" Offset="0.126"/>
                        <GradientStop Color="#FFFBF4C0" Offset="1"/>
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Ellipse x:Name="DisabledBackground" Opacity="0" >
                <Ellipse.Fill>
                    <RadialGradientBrush GradientOrigin="0.883,0.86">
                        <GradientStop Color="#FF797979" Offset="0.126"/>
                        <GradientStop Color="#FF919191" Offset="1"/>
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Ellipse x:Name="Glow" Opacity="0.65">
                <Ellipse.Fill>
                    <RadialGradientBrush GradientOrigin="0.243,0.087" RadiusY="0.632" RadiusX="0.632">
                        <GradientStop Color="White"/>
                        <GradientStop Offset="0.426"/>
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <ContentPresenter x:Name="contentPresenter" VerticalAlignment="Center" HorizontalAlignment="Center" 
                RenderTransformOrigin="0.5,0.5" SnapsToDevicePixels="True">
                <ContentPresenter.Effect>
                    <DropShadowEffect ShadowDepth="1" BlurRadius="2"/>
                </ContentPresenter.Effect>
                <ContentPresenter.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </ContentPresenter.RenderTransform>
            </ContentPresenter>
        </Grid>
    </ControlTemplate>


    <Style x:Key="MainButtonStyle" TargetType="Button">
