                            <Border x:Name="TrackBackground" BorderBrush="{StaticResource HorizontalSliderTrackNormalBorder}" BorderThickness="1" Background="{DynamicResource HorizontalSliderTrackNormalBackgroundRight}" CornerRadius="2" Height="4.0" Margin="5,0" Grid.Row="1" VerticalAlignment="center">

